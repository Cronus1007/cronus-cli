{
  "devDependencies": {
    "colors": "^1.4.0",
    "concurrently": "^6.2.0",
    "coveralls": "^3.1.0",
    "dayjs": "^1.10.5",
    "eslint": "^7.29.0",
    "istanbul-combine": "^0.3.0",
    "istanbul-merge": "^1.1.1",
    "jsdoc": "^3.6.7",
    "jsdoc-sphinx": "0.0.6",
    "lerna": "^4.0.0",
    "license-check-and-add": "^4.0.2",
    "semver": "^7.3.5",
    "sleep-promise": "^9.1.0",
    "source-map-support": "^0.5.19"
  },
  "engines": {
    "node": ">=12",
    "npm": ">=5"
  },
  "name": "cronus",
  "description": "You must install [Lerna](https://lernajs.io) to build this multi-package repository.",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "doc": "jsdoc -t ./node_modules/jsdoc-sphinx/template/ -c jsdoc.json -r -d ../cicero-docs/docs/api --verbose",
    "coverage": "istanbul-combine -d coverage -r lcov packages/cicero-cli/coverage/*.json packages/cicero-core/coverage/*.json  packages/cicero-engine/coverage/*.json packages/generator-cicero-template/coverage/*.json packages/cicero-server/coverage/*.json && cat ./coverage/lcov.info",
    "postinstall": "npm run bootstrap",
    "bootstrap": "lerna bootstrap",
    "pretest": "npm run doc && npm run licchk",
    "test": "lerna exec -- npm run test:cov",
    "repoclean": "lerna clean",
    "licchk": "license-check-and-add"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Cronus1007/cronus-cli"
  },
  "keywords": [
    "pem files",
    "digital signatures",
    "contract signing"
  ],
  "author": "Cronus1007",
  "license": "Apache-2.0",
  "license-check-and-add-config": "licchk",
  "dependencies": {}
}

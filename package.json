{
  "name": "@cronus1007/cronus-cli",
  "version": "1.0.0",
  "description": "Cronus CLI",
  "engines": {
    "node": ">=12",
    "npm": ">=5"
  },
  "publishConfig": {
    "access": "public"
  },
  "bin": {
    "cronus": "./index.js"
  },
  "main": "index.js",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "pretest": "npm run lint",
    "lint": "eslint .",
    "postlint": "npm run licchk",
    "licchk": "license-check",
    "test:cucumber": "cucumber-js test/features --require .cucumber.js --world-parameters \"{\\\"rootdir\\\":\\\"./test\\\"}\"",
    "test:mocha": "mocha",
    "test:all": "npm run test:mocha && npm run test:cucumber",
    "test:windows": "npm run test:all",
    "test:cov": "nyc npm run test",
    "test": "npm run test:all"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Cronus1007/cronus-cli.git"
  },
  "keywords": [
    "cli",
    "digital signatures",
    "legal",
    "tech"
  ],
  "author": "Cronus1007",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/Cronus1007/cronus-cli/issues"
  },
  "homepage": "https://github.com/Cronus1007/cronus-cli#readme",
  "dependencies": {
    "mkdirp": "^1.0.4",
    "uuid": "^8.3.2",
    "yargs": "^17.0.1"
  },
  "devDependencies": {
    "chai": "^4.3.4",
    "chai-as-promised": "^7.1.1",
    "chai-things": "^0.2.0",
    "cucumber": "^7.0.0-rc.0",
    "eslint": "^7.29.0",
    "jsdoc": "^3.6.7",
    "license-check": "^1.1.5",
    "mocha": "^9.0.1",
    "nyc": "^15.1.0",
    "tmp-promise": "^3.0.2"
  }
}
